e.innerHTML%20=%20html;return%20template.content.firstChild;}function%20adjacentSupCount(node)%20{let%20count%20=%200;while%20(node.nextSibling%20&&%20count%20<%205)%20{if%20(node.nextSibling.tagName%20===%20%22SUP%22)%20{count%20+=%201;}node%20=%20node.nextSibling;}return%20count;}function%20canModify(node)%20{return%20(node.nodeType%20===%203%20&&node.textContent.indexOf(%22.%22)%20!==%20-1%20&&adjacentSupCount(node)%20<%204%20&&Math.random()%20>%201%20-%200.9);}function%20addMarkup(para)%20{let%20i%20=%200;while%20(i%20<%20para.childNodes.length)%20{const%20node%20=%20para.childNodes[i];if%20(canModify(node))%20{node.splitText(node.textContent.indexOf(%22.%22)%20+%201);const%20comment%20=%20comments[Math.floor(Math.random()%20*%20comments.length)];const%20fragment%20=%20htmlToElement(comment);para.insertBefore(fragment,%20node.nextSibling);i%20+=%202;}i%20+=%201;}}Array.from(document.querySelector(%22#mw-content-text%22).querySelectorAll(%22p%22)).map((p)%20=>%20addMarkup(p));})();